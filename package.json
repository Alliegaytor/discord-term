{
    "name": "discord-term-ng",
    "version": "1.4.1",
    "description": "Extensible Discord terminal client",
    "exports": "./dist/index.js",
    "scripts": {
        "build": "tsc -skipLibCheck true",
        "start": "npm run build && node dist/index.js",
        "prepublish": "tsc",
        "test": "npm pack && sudo npm install discord-term-ng*.tgz -g && rm discord-term-ng*.tgz"
    },
    "keywords": [
        "discord",
        "discord-terminal",
        "discord-client",
        "discord-bot",
        "discordjs",
        "typescript",
        "blessed"
    ],
    "homepage": "https://gitlab.com/P-90-For-Retail/discord-term",
    "author": "Allison <alyway@protonmail.com>",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "https://gitlab.com/P-90-For-Retail/discord-term"
    },
    "dependencies": {
        "blessed": "^0.1.81",
        "chalk": "^5.2.0",
        "clipboardy": "^3.0.0",
        "discord.js": "^14.7.1",
        "string-width": "^5.1.2"
    },
    "devDependencies": {
        "@types/blessed": "^0.1.19",
        "@types/node": "^18.11.18",
        "typescript": "^4.9.4"
    },
    "files": [
        "dist/*.js",
        "dist/*/*.js",
        "themes"
    ],
    "bin": {
        "dterm": "dist/index.js"
    },
    "type": "module",
    "engines": {
        "node": ">=16.9.0"
    }
}
